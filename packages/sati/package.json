{
    "name": "sati",
    "version": "0.0.3",
    "description": "正念服务端",
    "scripts": {
        "start": "node dist/main.js",
        "dev": "ts-node -r tsconfig-paths/register src/main.ts",
        "check": "tslint -p tsconfig.json -c tslint.json",
        "fix": "tslint -p tsconfig.json -c tslint.json --fix",
        "build": "tsc;cp -fr src/* dist/*;",
        "build:docker": "npm run build;docker build -t sati -t sati:$(node -p \"require('./package.json').version\") ./"
    },
    "author": "jiangzhuo",
    "private": true,
    "dependencies": {
        "@alicloud/acm-sdk": "^1.0.1",
        "@grpc/proto-loader": "^0.3.0",
        "@nestjs/common": "^5.3.7",
        "@nestjs/core": "^5.3.7",
        "@nestjs/graphql": "^5.3.0",
        "@nestjs/microservices": "^5.3.7",
        "@sentry/node": "^4.3.0",
        "acm-client": "^3.1.0",
        "ali-oss": "^6.0.1",
        "apollo-server-express": "^2.0.6",
        "cache-manager": "^2.9.0",
        "collections": "^5.1.5",
        "express": "^4.16.4",
        "graphql": "^14.0.2",
        "graphql-tag": "^2.10.0",
        "graphql-tools": "^3.1.1",
        "graphql-type-json": "^0.2.1",
        "grpc": "^1.15.0",
        "hasha": "^3.0.0",
        "i18n": "^0.8.3",
        "jsonwebtoken": "^8.3.0",
        "mime": "^2.3.1",
        "moleculer": "^0.13.4",
        "moleculer-jaeger": "^0.2.0",
        "nacos": "^1.0.2",
        "nats": "^1.2.2",
        "nestjs-config": "^1.2.8",
        "nestjs-moleculer": "^1.0.5",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.3.2",
        "shimmer": "^1.2.0",
        "typescript": "^3.0.1",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/cache-manager": "^1.2.6",
        "@types/collections": "^5.0.0",
        "@types/graphql": "^14.0.7",
        "@types/graphql-type-json": "^0.1.2",
        "@types/hasha": "^3.0.0",
        "@types/i18n": "^0.8.3",
        "@types/jest": "^24.0.4",
        "@types/jsonwebtoken": "^7.2.8",
        "@types/mime": "^2.0.0",
        "@types/node": "^10.5.2",
        "@types/uuid": "^3.4.4",
        "ts-node": "^7.0.0",
        "tsconfig-paths": "^3.6.0",
        "tslint": "^5.11.0"
    }
}
